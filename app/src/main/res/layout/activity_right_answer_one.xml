<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="rightAnswerOneVM" type="com.tanze.app.modules.rightanswerone.data.viewmodel.RightAnswerOneVM"/>
    </data>
    <LinearLayout android:gravity="end"
        android:id="@+id/linearRightAnswerOn"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imageCloudshapeone"
            android:layout_height="@dimen/_58pxh"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_width="@dimen/_83pxh"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_cloudshapeone_orange_51}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_cloudshapeone_orange_51"/>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnohnoyoucan"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_28pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <TextView android:id="@+id/txtOhnoYoucan"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_26pxh"
                android:layout_marginStart="@dimen/_26pxh"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_11pxh"
                android:text="@{rightAnswerOneVM.rightAnswerOneModel.txtOhnoYoucan}"
                style="@style/txtPoppinsextrabold24" tools:text="@string/msg_oh_no_you_can"/>
            <ImageView android:gravity="center_horizontal"
                android:id="@+id/image3dicons"
                android:layout_height="@dimen/_327pxh"
                android:layout_marginEnd="@dimen/_26pxh"
                android:layout_marginStart="@dimen/_26pxh"
                android:layout_marginTop="@dimen/_73pxv"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_3dicons_327x327}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_3dicons_327x327"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowcloudshapeoneOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_127pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageCloudshapeoneOne"
                    android:layout_height="@dimen/_61pxh"
                    android:layout_width="@dimen/_64pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_cloudshapeone_orange_51}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_cloudshapeone_orange_51"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnTryAgain"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:layout_marginStart="@dimen/_31pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_weight="0.76"
                    android:layout_width="0dp"
                    android:paddingBottom="@dimen/_9pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_9pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_try_again"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded" tools:text="@string/lbl_try_again"/>
            </LinearLayout>
            <View android:background="@color/bluegray_100"
                android:id="@+id/viewRectangleFiftyNine"
                android:layout_gravity="start"
                android:layout_height="@dimen/_76pxv"
                android:layout_marginTop="@dimen/_17pxv" android:layout_width="@dimen/_390pxh"/>
        </LinearLayout>
    </LinearLayout>
</layout>
