                        android:background="@drawable/rectangle_bg_orange_53_radius_11"
                        android:ellipsize="end"
                        android:hint="@string/msg_select_your_reg"
                        android:id="@+id/etGroupTwentyOne"
                        android:inputType="text"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_25pxh"
                        android:layout_marginTop="@dimen/_25pxv"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_11pxh"
                        android:paddingBottom="@dimen/_10pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_20pxh"
                        android:paddingTop="@dimen/_13pxv"
                        android:singleLine="true"
                        android:text="@={rightAnswerTwoVM.rightAnswerTwoModel.etGroupTwentyOneValue}"
