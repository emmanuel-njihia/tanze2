                        android:lineSpacingExtra="@dimen/_11pxh"
                        android:paddingBottom="@dimen/_10pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_20pxh"
                        android:paddingTop="@dimen/_13pxv"
                        android:singleLine="true"
                        android:text="@={rightAnswerTwoVM.rightAnswerTwoModel.etGroupTwentyTwoValue}"
                        android:textColorHint="@color/gray_401"
                        style="@style/etSolidRounded_4" tools:ignore="Autofill"/>
