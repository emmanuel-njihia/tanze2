<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="countrySelectionVM" type="com.tanze.app.modules.countryselection.data.viewmodel.CountrySelectionVM"/>
    </data>
    <FrameLayout android:id="@+id/frameCountrySelecti"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnwhereareyouf"
            android:layout_gravity="top|center"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_55pxv"
            android:layout_marginEnd="@dimen/_31pxh"
            android:layout_marginStart="@dimen/_31pxh"
            android:layout_marginTop="@dimen/_55pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <ImageButton
                android:background="@drawable/rectangle_bg_lime_900_radius_24_5"
                android:id="@+id/btnBackButton"
                android:layout_height="@dimen/_49pxh"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="@dimen/_49pxh"
                android:paddingHorizontal="@dimen/_8pxh"
                android:paddingVertical="@dimen/_8pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_backbutton" tools:ignore="ContentDescription"/>
            <TextView android:id="@+id/txtWhereareyouf"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_4pxh"
                android:layout_marginTop="@dimen/_27pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_5pxh"
                android:text="@{countrySelectionVM.countrySelectionModel.txtWhereareyouf}"
                style="@style/txtPoppinsextrabold30" tools:text="@string/msg_where_are_you_f"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSelectYourCou"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_4pxh"
                android:layout_marginTop="@dimen/_28pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_10pxh"
                android:singleLine="true"
                android:text="@{countrySelectionVM.countrySelectionModel.txtSelectYourCou}"
                style="@style/txtPoppinsmedium20" tools:text="@string/msg_select_your_cou"/>
            <LinearLayout android:gravity="end"
                android:id="@+id/linearRowclose"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_61pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnclose"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_weight="0.98"
                    android:layout_width="0dp" android:orientation="vertical">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearKenya"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageClose"
                            android:layout_height="@dimen/_15pxh"
                            android:layout_marginBottom="@dimen/_9pxv"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_width="@dimen/_21pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_close"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_close"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtKenya"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_25pxh"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_10pxh"
                            android:singleLine="true"
                            android:text="@{countrySelectionVM.countrySelectionModel.txtKenya}"
                            style="@style/txtPoppinsmedium20_1" tools:text="@string/lbl_kenya"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowticket"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageTicket"
                            android:layout_height="@dimen/_15pxh"
                            android:layout_marginBottom="@dimen/_9pxv"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_width="@dimen/_21pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_ticket"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_ticket"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtUganda"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_25pxh"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_10pxh"
                            android:singleLine="true"
                            android:text="@{countrySelectionVM.countrySelectionModel.txtUganda}"
                            style="@style/txtPoppinsmedium20_1" tools:text="@string/lbl_uganda"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowclippathgroup"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageClippathgroup"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_marginBottom="@dimen/_6pxv"
                            android:layout_marginTop="@dimen/_7pxv"
                            android:layout_width="@dimen/_21pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_clippathgroup"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_clippathgroup"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTanzania"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_10pxh"
                            android:singleLine="true"
                            android:text="@{countrySelectionVM.countrySelectionModel.txtTanzania}"
                            style="@style/txtPoppinsmedium20_1" tools:text="@string/lbl_tanzania"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowsettings"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageSettings"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_marginBottom="@dimen/_8pxv"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_width="@dimen/_21pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_settings"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_settings"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtEthiopia"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_10pxh"
                            android:singleLine="true"
                            android:text="@{countrySelectionVM.countrySelectionModel.txtEthiopia}"
                            style="@style/txtPoppinsmedium20_1" tools:text="@string/lbl_ethiopia"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowiconrwanda"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageIconRwanda"
                            android:layout_height="@dimen/_15pxh"
                            android:layout_marginBottom="@dimen/_6pxv"
                            android:layout_marginTop="@dimen/_7pxv"
                            android:layout_width="@dimen/_21pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_iconrwanda"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_iconrwanda"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtRwanda"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_25pxh"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_10pxh"
                            android:singleLine="true"
                            android:text="@{countrySelectionVM.countrySelectionModel.txtRwanda}"
                            style="@style/txtPoppinsmedium20_1" tools:text="@string/lbl_rwanda"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowcloseOne"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageCloseOne"
                            android:layout_height="@dimen/_15pxh"
                            android:layout_marginBottom="@dimen/_6pxv"
                            android:layout_marginTop="@dimen/_7pxv"
                            android:layout_width="@dimen/_21pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_close_15x21"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_close_15x21"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtBurundi"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_25pxh"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_10pxh"
                            android:singleLine="true"
                            android:text="@{countrySelectionVM.countrySelectionModel.txtBurundi}"
                            style="@style/txtPoppinsmedium20_1" tools:text="@string/lbl_burundi"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowiconsouth"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageIconSouth"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_marginBottom="@dimen/_6pxv"
                            android:layout_marginTop="@dimen/_7pxv"
                            android:layout_width="@dimen/_21pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_iconsouth"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_iconsouth"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtCountry"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_10pxh"
                            android:singleLine="true"
                            android:text="@{countrySelectionVM.countrySelectionModel.txtCountry}"
                            style="@style/txtPoppinsmedium20_1" tools:text="@string/lbl_sudan"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowvector"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <FrameLayout android:id="@+id/frameStackvector"
                            android:layout_height="@dimen/_15pxv"
                            android:layout_marginBottom="@dimen/_9pxv"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_width="@dimen/_21pxh" style="@style/groupStylewhite_A700">
                            <ImageView android:id="@+id/imageVector"
                                android:layout_gravity="start"
                                android:layout_height="@dimen/_15pxh"
                                android:layout_width="@dimen/_21pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_vector"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
                        </FrameLayout>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtNigeria"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_25pxh"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_10pxh"
                            android:singleLine="true"
                            android:text="@{countrySelectionVM.countrySelectionModel.txtNigeria}"
                            style="@style/txtPoppinsmedium20_1" tools:text="@string/lbl_nigeria"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowclippathgroupOne"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView
                            android:id="@+id/imageClippathgroupOne"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_marginBottom="@dimen/_6pxv"
                            android:layout_marginTop="@dimen/_7pxv"
                            android:layout_width="@dimen/_21pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_clippathgroup_16x21"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_clippathgroup_16x21"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtSouthAfrica"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_25pxh"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_10pxh"
                            android:singleLine="true"
                            android:text="@{countrySelectionVM.countrySelectionModel.txtSouthAfrica}"
                            style="@style/txtPoppinsmedium20_1" tools:text="@string/lbl_south_africa"/>
                    </LinearLayout>
                </LinearLayout>
                <FrameLayout
                    android:id="@+id/frameStackrectangletwentyfour"
                    android:layout_height="@dimen/_203pxv"
                    android:layout_marginBottom="@dimen/_143pxv" android:layout_width="@dimen/_5pxh">
                    <View
                        android:background="@drawable/rectangle_bg_lime_900_radius_2_5"
                        android:id="@+id/lineRectangleTwentyFour"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_203pxv" android:layout_width="@dimen/_5pxh"/>
                    <View
                        android:background="@drawable/rectangle_bg_white_a700_border_black_900_radius_2_5"
                        android:id="@+id/lineRectangleTwentyFive"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_64pxv"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginTop="@dimen/_3pxv" android:layout_width="@dimen/_5pxh"/>
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
