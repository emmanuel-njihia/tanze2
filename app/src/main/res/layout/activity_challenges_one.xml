<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="challengesOneVM" type="com.tanze.app.modules.challengesone.data.viewmodel.ChallengesOneVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumncloudshapeone"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearRowcloudshapeone"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <FrameLayout android:id="@+id/frameStackcloudshapeone"
                    android:layout_height="@dimen/_102pxv" android:layout_width="@dimen/_75pxh">
                    <ImageView android:id="@+id/imageCloudshapeone"
                        android:layout_gravity="top|center"
                        android:layout_height="@dimen/_54pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_1pxh"
                        android:layout_width="@dimen/_74pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_cloudshapeone_orange_51}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_cloudshapeone_orange_51"/>
                    <ImageButton
                        android:background="@drawable/rectangle_bg_lime_900_radius_24_5"
                        android:id="@+id/btnBackButton"
                        android:layout_gravity="bottom|end"
                        android:layout_height="@dimen/_49pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_49pxh"
                        android:paddingHorizontal="@dimen/_8pxh"
                        android:paddingVertical="@dimen/_8pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_backbutton" tools:ignore="ContentDescription"/>
                </FrameLayout>
                <ImageView android:id="@+id/imageEllipseFour"
                    android:layout_height="@dimen/_49pxh"
                    android:layout_marginStart="@dimen/_237pxh"
                    android:layout_marginTop="@dimen/_53pxv"
                    android:layout_width="@dimen/_49pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_ellipse4}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse4"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearChallengesOne"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_29pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <TextView android:ellipsize="end"
                android:id="@+id/txtActivitiesFor"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_26pxh"
                android:layout_marginStart="@dimen/_26pxh"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_15pxh"
                android:singleLine="true"
                android:text="@{challengesOneVM.challengesOneModel.txtActivitiesFor}"
                style="@style/txtPoppinsextrabold30" tools:text="@string/msg_activities_for"/>
            <LinearLayout android:id="@+id/linearRowlearnaboutind"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStyleamber_2">
                <TextView android:id="@+id/txtLearnaboutInd"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_weight="0.78"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:text="@{challengesOneVM.challengesOneModel.txtLearnaboutInd}"
                    style="@style/txtPoppinssemibold20" tools:text="@string/msg_learn_about_in"/>
                <View
                    android:background="@drawable/rectangle_bg_white_a700_radius_10"
                    android:id="@+id/viewRectangleSeventyFour"
                    android:layout_height="@dimen/_50pxh"
                    android:layout_marginBottom="@dimen/_18pxv"
                    android:layout_marginEnd="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_76pxh"
                    android:layout_marginTop="@dimen/_19pxv" android:layout_width="@dimen/_50pxh"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowshowyourtree"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_23pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylegreen_1">
                <TextView android:id="@+id/txtShowyourtree"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_16pxv"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_weight="0.83"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:text="@{challengesOneVM.challengesOneModel.txtShowyourtree}"
                    style="@style/txtPoppinssemibold20" tools:text="@string/msg_show_your_tree"/>
                <View
                    android:background="@drawable/rectangle_bg_white_a700_radius_10"
                    android:id="@+id/viewRectangleSeventyFive"
                    android:layout_height="@dimen/_50pxh"
                    android:layout_marginBottom="@dimen/_18pxv"
                    android:layout_marginEnd="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_marginTop="@dimen/_19pxv" android:layout_width="@dimen/_50pxh"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnrectangleseventysix"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_23pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylered_1">
                <View
                    android:background="@drawable/rectangle_bg_white_a700_radius_10"
                    android:id="@+id/viewRectangleSeventySix"
                    android:layout_gravity="end"
                    android:layout_height="@dimen/_50pxh"
                    android:layout_marginBottom="@dimen/_18pxv"
                    android:layout_marginEnd="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_19pxv" android:layout_width="@dimen/_50pxh"/>
            </LinearLayout>
            <View android:background="@color/bluegray_100"
                android:id="@+id/viewRectangleFiftyNine"
                android:layout_height="@dimen/_76pxv"
                android:layout_marginTop="@dimen/_338pxv" android:layout_width="@dimen/_390pxh"/>
        </LinearLayout>
    </LinearLayout>
</layout>
