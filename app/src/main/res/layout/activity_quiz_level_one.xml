<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="quizLevelOneVM" type="com.tanze.app.modules.quizlevelone.data.viewmodel.QuizLevelOneVM"/>
    </data>
    <LinearLayout android:gravity="end"
        android:id="@+id/linearQuizLevelOne"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imageCloudshapeone"
            android:layout_height="@dimen/_58pxh"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_width="@dimen/_83pxh"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_cloudshapeone_orange_51}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_cloudshapeone_orange_51"/>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnknowtreesink"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_78pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtKnowTreesinK"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_5pxh"
                android:text="@{quizLevelOneVM.quizLevelOneModel.txtKnowTreesinK}"
                style="@style/txtPoppinsextrabold30" tools:text="@string/msg_know_trees_in_k"/>
            <FrameLayout android:id="@+id/frameStacklightbulb"
                android:layout_gravity="end"
                android:layout_height="@dimen/_78pxv"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_13pxv" android:layout_width="@dimen/_374pxh">
                <LinearLayout android:id="@+id/linearRowlightbulb"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStyleorange_1">
                    <ImageView android:id="@+id/imageLightbulb"
                        android:layout_height="@dimen/_27pxh"
                        android:layout_marginBottom="@dimen/_23pxv"
                        android:layout_marginStart="@dimen/_57pxh"
                        android:layout_marginTop="@dimen/_21pxv"
                        android:layout_width="@dimen/_31pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_lightbulb"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_lightbulb"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtYoureat10Po"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_22pxv"
                        android:layout_marginEnd="@dimen/_75pxh"
                        android:layout_marginStart="@dimen/_9pxh"
                        android:layout_marginTop="@dimen/_23pxv"
                        android:layout_weight="0.86"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_5pxh"
                        android:singleLine="true"
                        android:text="@{quizLevelOneVM.quizLevelOneModel.txtYoureat10Po}"
                        style="@style/txtMontserratromanmedium20" tools:text="@string/msg_you_re_at_10_po"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageVectorThree"
                    android:layout_gravity="top|end"
                    android:layout_height="@dimen/_50pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_width="@dimen/_81pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_vector2}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector2"/>
            </FrameLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowrectanglesixty"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_45pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageRectangleSixty"
                    android:layout_height="@dimen/_57pxh"
                    android:layout_width="@dimen/_57pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_25pxh}"
                    imageRes="@{@drawable/img_rectangle60}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle60"/>
                <ImageView android:id="@+id/imageRectangleSixtyThree"
                    android:layout_height="@dimen/_57pxh"
                    android:layout_marginStart="@dimen/_21pxh"
                    android:layout_width="@dimen/_57pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_25pxh}"
                    imageRes="@{@drawable/img_rectangle60}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle60"/>
                <FrameLayout android:id="@+id/frameQuizOption"
                    android:layout_height="@dimen/_55pxv"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_23pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_61pxh" style="@style/groupStylered_51">
                    <ImageView android:id="@+id/imageLock"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_13pxh"
                        android:layout_marginBottom="@dimen/_21pxv"
                        android:layout_marginEnd="@dimen/_25pxh"
                        android:layout_marginStart="@dimen/_25pxh"
                        android:layout_marginTop="@dimen/_21pxv"
                        android:layout_width="@dimen/_11pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_lock"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_lock"/>
                </FrameLayout>
                <FrameLayout android:id="@+id/frameQuizOption1"
                    android:layout_height="@dimen/_55pxv"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_19pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_61pxh" style="@style/groupStylered_51">
                    <ImageView android:id="@+id/imageLockOne"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_13pxh"
                        android:layout_marginBottom="@dimen/_21pxv"
                        android:layout_marginEnd="@dimen/_25pxh"
                        android:layout_marginStart="@dimen/_25pxh"
                        android:layout_marginTop="@dimen/_21pxv"
                        android:layout_width="@dimen/_11pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_lock"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_lock"/>
                </FrameLayout>
            </LinearLayout>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="center_horizontal"
                android:id="@+id/recyclerQuizLevelOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_25pxv"
                android:layout_width="match_parent"
                app:layoutManager="androidx.recyclerview.widget.GridLayoutManager"
                app:spanCount="4" itemSpace="@{@dimen/_19pxh}"
                tools:itemCount="12" tools:listitem="@layout/row_quiz_level_one"/>
            <ImageView android:id="@+id/imageCloudshapeoneOne"
                android:layout_gravity="start"
                android:layout_height="@dimen/_61pxh"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_40pxv"
                android:layout_width="@dimen/_44pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_cloudshapeone_orange_51"
                tools:ignore="ContentDescription" tools:src="@drawable/img_cloudshapeone_orange_51"/>
            <View android:background="@color/bluegray_100"
                android:id="@+id/viewRectangleFiftyNine"
                android:layout_gravity="start"
                android:layout_height="@dimen/_76pxv"
                android:layout_marginTop="@dimen/_17pxv" android:layout_width="@dimen/_390pxh"/>
        </LinearLayout>
    </LinearLayout>
</layout>
